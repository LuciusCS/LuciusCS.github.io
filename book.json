  "description": "Android开发全纪录",
  "language": "zh-hans",
    "plugins": [
    "theme-comscore",
	"prism",
    "-highlight",
    "copy-code-button",
    "search-pro",
    "-search",
	"-lunr",
    "expandable-chapters",
    "splitter",
    "-sharing",
    "github-buttons",
    "tbfed-pagefooter",
	"anchor-navigation-ex"
